ge.getItem(\"cart\"));\r\n    }\r\n\r\n    cart.map((product, index) => {\r\n      if (product._id === id) {\r\n        cart[index].count = count;\r\n      }\r\n    });\r\n\r\n    localStorage.setItem(\"cart\", JSON.stringify(cart));\r\n  }\r\n};\r\n\r\nexport const removeItem = (id) => {\r\n  let cart = [];\r\n  if (typeof window != \"undefined\") {\r\n    if (localStorage.getItem(\"cart\")) {\r\n      cart = JSON.parse(localStorage.getItem(\"cart\"));\r\n    }\r\n\r\n    cart.map((product, index) => {\r\n      if (product._id === id) {\r\n        cart.splice(index, 1);\r\n      }\r\n    });\r\n\r\n    localStorage.setItem(\"cart\", JSON.stringify(cart));\r\n  }\r\n  return cart;\r\n};\r\n","C:\\Users\\prana\\OneDrive\\Desktop\\React\\ecommerce-front\\src\\core\\Cart.js",[],"C:\\Users\\prana\\OneDrive\\Desktop\\React\\ecommerce-front\\src\\core\\Checkout.js",["147","148","149","150","151"],{"ruleId":"152","replacedBy":"153"},{"ruleId":"154","replacedBy":"155"},{"ruleId":"156","severity":1,"message":"157","line":11,"column":10,"nodeType":"158","messageId":"159","endLine":11,"endColumn":24},{"ruleId":"156","severity":1,"message":"160","line":13,"column":10,"nodeType":"158","messageId":"159","endLine":13,"endColumn":15},{"ruleId":"156","severity":1,"message":"161","line":15,"column":10,"nodeType":"158","messageId":"159","endLine":15,"endColumn":16},{"ruleId":"156","severity":1,"message":"162","line":30,"column":5,"nodeType":"158","messageId":"159","endLine":30,"endColumn":13},{"ruleId":"156","severity":1,"message":"163","line":31,"column":5,"nodeType":"158","messageId":"159","endLine":31,"endColumn":13},{"ruleId":"156","severity":1,"message":"164","line":36,"column":5,"nodeType":"158","messageId":"159","endLine":36,"endColumn":22},{"ruleId":"165","severity":1,"message":"166","line":57,"column":6,"nodeType":"167","endLine":57,"endColumn":8,"suggestions":"168"},{"ruleId":"152","replacedBy":"169"},{"ruleId":"154","replacedBy":"170"},{"ruleId":"156","severity":1,"message"